# Upstream Repos
[rocky-extras-$releasever.$basearch]
name = Rocky Linux $releasever Extras ($basearch)
baseurl = ${baseurl1}/public/files/upstream-$releasever/rocky-extras-$releasever.$basearch,${baseurl2}/public/files/upstream-$releasever/rocky-extras-$releasever.$basearch
gpgkey = https://dl.rockylinux.org/pub/rocky/RPM-GPG-KEY-Rocky-$releasever
priority = 50
gpgcheck = true
enabled = true

[rocky-baseos-$releasever.$basearch]
name = Rocky Linux $releasever BaseOS ($basearch)
baseurl = ${baseurl1}/public/files/upstream-$releasever/rocky-baseos-$releasever.$basearch,${baseurl2}/public/files/upstream-$releasever/rocky-baseos-$releasever.$basearch
gpgkey = https://dl.rockylinux.org/pub/rocky/RPM-GPG-KEY-Rocky-$releasever
priority = 50
gpgcheck = true
enabled = true
#exclude = kernel*

[rocky-appstream-$releasever.$basearch]
